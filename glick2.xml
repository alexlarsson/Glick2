<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-glick-bundle">
     <sub-class-of type="application/x-executable"/>
     <comment>Glick2 bundle</comment>
     <generic-icon name="application-x-executable"/>
     <glob pattern="*.bundle"/>
     <magic priority="50">
       <match type="string" value="\x8cGLK\x0d\x0a\x1a\x0a" offset="0"/>
     </magic>
   </mime-type>
</mime-info>
